#!/usr/bin/env zsh

cd "$(dirname $0)"/../packages

echo ''; hr - '#' -
echo "[SYMLINKING]\n"

# Make sure that .stow-global-ignore is set
stow -R -t $HOME stow

BASEFILES=(
	ack
	curl
	editorconfig
	fzf
	git
	# htop
	nano
	prezto
	ruby
	shell
	wget
	# tmux
	zsh
)
stow -Rv -t $HOME $BASEFILES

# dotp-symlink
